import java.util.*;
import java.util.Scanner;
class Sample
{
    public static void main(String[]args)
    {
        Scanner sc=new Scanner(System.in);
char ch='A';
int n=sc.nextInt();

        for(int i=1;i<=n;i++)
        {
            for(int j=1;j<=i;j++)
            {
                     System.out.print(ch++);
            }
        
        System.out.println("");
        
        }
    }
}

Output
5
A
BC
DEF
GHIJ
KLMNO
